<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/black"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/content_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/programs_bottom_layout"
        android:layout_alignParentTop="true"/>
    <LinearLayout
        android:gravity="center"
        android:orientation="vertical"
        android:id="@+id/programs_bottom_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:layout_alignParentBottom="true">
        <TableLayout
            android:id="@+id/programs_tablelayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <TextView
            android:textSize="@dimen/profiles_info_text_size"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@+id/begin_info_textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/begin_start_info"/>
    </LinearLayout>
</RelativeLayout>
