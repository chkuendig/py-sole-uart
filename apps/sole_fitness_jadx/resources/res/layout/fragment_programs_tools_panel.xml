<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/program_menu_sole"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/btnOutdoor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Add/Edit Goal"/>
        <Button
            android:id="@+id/btnExportDB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Export DB"/>
        <Button
            android:id="@+id/btn2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="btn2"
            android:layout_weight="1"/>
        <Button
            android:id="@+id/btn3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="btn3"
            android:layout_weight="1"/>
        <Button
            android:id="@+id/btn4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="btn4"
            android:layout_weight="1"/>
        <Button
            android:id="@+id/btn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="btn1"
            android:layout_weight="1"/>
    </LinearLayout>
    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView1"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/txt_src_db_path"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView2"/>
        <TextView
            android:id="@+id/txt_dst_db_path"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView3"/>
        <TextView
            android:id="@+id/txt_adb_cmd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView4"/>
        <TextView
            android:textColor="@android:color/holo_blue_dark"
            android:id="@+id/txt_adb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView5"/>
    </LinearLayout>
</LinearLayout>
