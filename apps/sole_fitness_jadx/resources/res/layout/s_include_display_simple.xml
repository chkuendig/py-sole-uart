<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include
            android:id="@+id/display_simple_left_1_include"
            android:layout_width="0dp"
            android:layout_height="@dimen/s_display_dashboard_top_height"
            android:layout_weight="1"
            layout="@layout/s_include_display_simple_item"/>
        <View
            android:background="@color/light_gray"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <include
            android:id="@+id/display_simple_left_2_include"
            android:layout_width="0dp"
            android:layout_height="@dimen/s_display_dashboard_top_height"
            android:layout_weight="1"
            layout="@layout/s_include_display_simple_item"/>
        <View
            android:background="@color/light_gray"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <include
            android:id="@+id/display_simple_left_3_include"
            android:layout_width="0dp"
            android:layout_height="@dimen/s_display_dashboard_top_height"
            android:layout_weight="1"
            layout="@layout/s_include_display_simple_item"/>
        <View
            android:background="@color/light_gray"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <include
            android:id="@+id/display_simple_left_4_include"
            android:layout_width="0dp"
            android:layout_height="@dimen/s_display_dashboard_top_height"
            android:layout_weight="1"
            layout="@layout/s_include_display_simple_item"/>
        <View
            android:background="@color/light_gray"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <include
            android:id="@+id/display_simple_left_5_include"
            android:layout_width="0dp"
            android:layout_height="@dimen/s_display_dashboard_top_height"
            android:layout_weight="1"
            layout="@layout/s_include_display_simple_item"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/s_display_simple_margin"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:layout_marginStart="@dimen/s_display_simple_margin"
        android:layout_marginEnd="@dimen/s_display_simple_margin">
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/display_simple_left_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginEnd="10dp">
            <com.dyaco.sole.custom_view.TypefaceTextView
                android:textSize="@dimen/s_main_menu_text_size"
                android:textColor="@color/black"
                android:gravity="center"
                android:id="@+id/display_simple_muscle_text"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/muscle_profile"/>
            <ImageView
                android:id="@+id/display_simple_muscle_image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:src="@drawable/s_display_ef_1"
                android:layout_marginEnd="@dimen/s_display_simple_margin"/>
        </LinearLayout>
        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginStart="10dp">
            <RelativeLayout
                android:orientation="vertical"
                android:id="@+id/display_simple_right_1_layout"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <com.dyaco.sole.custom_view.TypefaceTextView
                    android:textSize="@dimen/s_main_menu_text_size"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:id="@+id/display_simple_profile_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/active_profile_level"/>
                <RelativeLayout
                    android:id="@+id/display_simple_profile_layout"
                    android:layout_width="@dimen/s_display_simple_profile_width"
                    android:layout_height="@dimen/s_display_simple_profile_height"
                    android:layout_centerInParent="true"/>
            </RelativeLayout>
            <FrameLayout
                android:id="@+id/display_simple_right_2_layout"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/s_display_simple_hr"/>
                <com.dyaco.sole.custom_view.TypefaceTextView
                    android:textSize="13sp"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:id="@+id/target_hr_text"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/simple_hr_info"
                    android:layout_marginStart="25dp"
                    android:layout_marginEnd="25dp"/>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</LinearLayout>
