<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/fragment_history_workout_0"
    android:background="@color/colorBackground_canvas"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/statusBarHeight"
        android:background="@color/colorBackground_canvas"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:background="@color/colorBackground_canvas"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusBarHeight">
        <com.google.android.material.appbar.AppBarLayout
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            android:id="@+id/appBarLayout"
            android:background="@color/colorBackground_canvas"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.google.android.material.appbar.CollapsingToolbarLayout
                android:background="@color/colorBackground_canvas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_scrollFlags="exitUntilCollapsed|scroll">
                <LinearLayout
                    android:orientation="vertical"
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/CL_isBest"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="225dp">
                        <ImageView
                            android:id="@+id/img_best"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:scaleType="centerCrop"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/bike_medium"/>
                        <LinearLayout
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">
                            <ImageView
                                android:id="@+id/img"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:srcCompat="@drawable/ic_applaud"
                                app:tint="@color/colorGlobal_white"/>
                            <TextView
                                android:textSize="@dimen/text_size_17"
                                android:textColor="@color/colorGlobal_white"
                                android:id="@+id/tv_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/margin_12"
                                android:text="@string/best_5k_run"
                                android:fontFamily="@font/inter_semibold_600"/>
                            <TextView
                                android:textSize="@dimen/text_size_13"
                                android:id="@+id/tv_titleDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/margin_4"
                                android:text="-"
                                android:fontFamily="@font/inter_medium_500"/>
                        </LinearLayout>
                        <ImageView
                            android:id="@+id/img_back"
                            android:background="@drawable/bg_corner28_black_overlay_50"
                            android:padding="@dimen/padding_8"
                            android:layout_width="@dimen/img_size_44"
                            android:layout_height="@dimen/img_size_44"
                            android:layout_marginTop="@dimen/margin_12"
                            android:layout_marginStart="@dimen/margin_12"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/ic_m__xmark"
                            app:tint="@color/colorGlobal_white"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <TextView
                        android:textSize="34sp"
                        android:textColor="@color/colorLabel_label1"
                        android:id="@+id/tv_header"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_64"
                        android:text="@string/treadmill_workout"
                        android:fontFamily="@font/inter_extrabold_800"
                        android:layout_marginStart="@dimen/margin_20"/>
                </LinearLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/collapsing_toolbar"
                    android:background="@color/colorBackground_canvas"
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/top_bar_total_height"
                    app:contentInsetLeft="0dp"
                    app:contentInsetStart="0dp"
                    app:layout_collapseMode="parallax"
                    app:layout_scrollFlags="exitUntilCollapsed"
                    app:titleMargin="0dp">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/topbar"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/top_bar_total_height"
                        app:layout_collapseMode="parallax"
                        app:layout_scrollFlags="exitUntilCollapsed">
                        <ImageView
                            android:id="@+id/back"
                            android:padding="@dimen/padding_8"
                            android:layout_width="@dimen/img_size_44"
                            android:layout_height="@dimen/img_size_44"
                            android:layout_marginStart="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/ic_m__xmark"/>
                        <ImageView
                            android:id="@+id/endImg"
                            android:padding="@dimen/padding_8"
                            android:visibility="invisible"
                            android:layout_width="@dimen/img_size_44"
                            android:layout_height="@dimen/img_size_44"
                            android:layout_marginEnd="8dp"
                            app:layout_constraintBottom_toTopOf="@+id/view2"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/ic_search"/>
                        <TextView
                            android:textSize="@dimen/text_size_17"
                            android:textColor="@color/colorLabel_label1"
                            android:id="@+id/title"
                            android:visibility="invisible"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/treadmill_workout"
                            android:fontFamily="@font/inter_semibold_600"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <View
                            android:id="@+id/view2"
                            android:background="@color/colorLabel_outline"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.appcompat.widget.Toolbar>
            </com.google.android.material.appbar.CollapsingToolbarLayout>
        </com.google.android.material.appbar.AppBarLayout>
        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_16"
                    android:paddingStart="@dimen/padding_20"
                    android:paddingEnd="@dimen/padding_20">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:background="@drawable/bg_corner24_raised"
                        android:paddingTop="@dimen/padding_12"
                        android:paddingBottom="@dimen/padding_12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_8"
                        android:paddingStart="@dimen/padding_16"
                        android:paddingEnd="@dimen/padding_16">
                        <LinearLayout
                            android:gravity="top"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:textSize="@dimen/text_size_14"
                                android:textColor="@color/colorLabel_label1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/date"
                                android:textAllCaps="true"
                                android:fontFamily="@font/inter_bold_700"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1">
                            <TextView
                                android:textSize="@dimen/text_size_17"
                                android:textColor="@color/colorLabel_label1"
                                android:gravity="bottom"
                                android:id="@+id/txt_date"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="-"
                                android:includeFontPadding="false"
                                android:lineSpacingExtra="4dp"
                                android:fontFamily="@font/inter_regular_400"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/padding_12"
                        android:paddingBottom="@dimen/padding_12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_8"
                        android:paddingStart="@dimen/padding_16"
                        android:paddingEnd="@dimen/padding_16">
                        <LinearLayout
                            android:gravity="top"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:textSize="@dimen/text_size_14"
                                android:textColor="@color/colorLabel_label1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/time"
                                android:textAllCaps="true"
                                android:fontFamily="@font/inter_bold_700"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1">
                            <LinearLayout
                                android:gravity="top"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">
                                <TextView
                                    android:textSize="@dimen/text_size_17"
                                    android:textColor="@color/colorLabel_label1"
                                    android:gravity="bottom"
                                    android:id="@+id/time"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="08:45 AM â€”\1009:05 AM"
                                    android:includeFontPadding="false"
                                    android:lineSpacingExtra="4dp"
                                    android:fontFamily="@font/inter_regular_400"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:background="@drawable/bg_corner24_raised"
                        android:paddingTop="@dimen/padding_12"
                        android:paddingBottom="@dimen/padding_12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_8"
                        android:paddingStart="@dimen/padding_16"
                        android:paddingEnd="@dimen/padding_16">
                        <LinearLayout
                            android:gravity="top"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <TextView
                                android:textSize="@dimen/text_size_14"
                                android:textColor="@color/colorLabel_label1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/workout"
                                android:textAllCaps="true"
                                android:fontFamily="@font/inter_bold_700"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1">
                            <LinearLayout
                                android:gravity="top"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1">
                                <TextView
                                    android:textSize="@dimen/text_size_17"
                                    android:textColor="@color/colorLabel_label1"
                                    android:gravity="bottom"
                                    android:id="@+id/workoutName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:includeFontPadding="false"
                                    android:lineSpacingExtra="4dp"
                                    android:fontFamily="@font/inter_regular_400"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/LL_workoutInfo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_32"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_32"
                    android:layout_marginBottom="@dimen/margin_20"
                    android:layout_marginHorizontal="@dimen/margin_20">
                    <TextView
                        android:textSize="@dimen/text_size_21"
                        android:textColor="@color/colorLabel_label1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/details"
                        android:fontFamily="@font/inter_extrabold_800"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/LL_chart"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/LL_pace"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_32"
                    android:layout_marginBottom="@dimen/margin_20"
                    android:layout_marginHorizontal="@dimen/margin_20">
                    <TextView
                        android:textSize="@dimen/text_size_21"
                        android:textColor="@color/colorLabel_label1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/splits"
                        android:fontFamily="@font/inter_extrabold_800"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_pace"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/margin_12"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
    <View
        android:id="@+id/view4"
        android:background="@color/colorLabel_outline"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintBottom_toTopOf="@+id/LL_bottom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/LL_bottom"
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:paddingStart="@dimen/padding_20"
        android:paddingEnd="@dimen/padding_20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">
        <LinearLayout
            android:gravity="start|center_vertical"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/img_delete"
                android:padding="@dimen/padding_8"
                android:layout_width="@dimen/img_size_44"
                android:layout_height="@dimen/img_size_44"
                android:layout_marginStart="@dimen/margin_8"
                app:srcCompat="@drawable/ic_m__trash"
                app:tint="@color/colorGlobal_accent"/>
        </LinearLayout>
        <View
            android:id="@+id/space1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <LinearLayout
            android:gravity="start|center_vertical"
            android:orientation="horizontal"
            android:id="@+id/LL_share"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <ImageView
                android:padding="@dimen/padding_8"
                android:layout_width="@dimen/img_size_44"
                android:layout_height="@dimen/img_size_44"
                android:layout_marginStart="@dimen/margin_8"
                app:srcCompat="@drawable/ic_m__share"
                app:tint="@color/colorGlobal_accent"/>
            <TextView
                android:textSize="@dimen/text_size_17"
                android:textColor="@color/colorGlobal_accent"
                android:gravity="center"
                android:id="@+id/tv_share"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/share"
                android:includeFontPadding="false"
                android:fontFamily="@font/inter_semibold_600"/>
        </LinearLayout>
        <View
            android:id="@+id/space2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/LL_rematch"
                android:background="@drawable/bg_corner32_accent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:foreground="?attr/selectableItemBackgroundBorderless">
                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_24"
                    app:srcCompat="@drawable/ic_m__repeat"
                    app:tint="@color/colorGlobal_white"/>
                <TextView
                    android:textSize="@dimen/text_size_17"
                    android:textColor="@color/colorGlobal_white"
                    android:ellipsize="end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_16"
                    android:layout_marginBottom="@dimen/margin_16"
                    android:text="@string/rematch"
                    android:maxLines="1"
                    android:fontFamily="@font/inter_semibold_600"
                    android:layout_marginStart="@dimen/margin_8"
                    android:layout_marginEnd="@dimen/margin_24"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
