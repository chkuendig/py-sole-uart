<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/adapter_history_header_item_0"
    android:background="@color/colorBackground_canvas"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/margin_20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:textSize="@dimen/text_size_21"
            android:textColor="@color/colorLabel_label1"
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="July 2024"
            android:fontFamily="@font/inter_extrabold_800"/>
        <TextView
            android:textSize="@dimen/text_size_15"
            android:textColor="@color/colorLabel_label2"
            android:id="@+id/tv_workoutsCount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/workouts_count"
            android:fontFamily="@font/inter_medium_500"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
