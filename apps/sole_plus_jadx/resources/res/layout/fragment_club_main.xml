<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipeRefresh"
    android:tag="layout/fragment_club_main_0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <View
            android:id="@+id/statusBarHeight"
            android:layout_width="match_parent"
            android:layout_height="0dp"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/tobBar"
                android:layout_width="0dp"
                android:layout_height="68dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <TextView
                    android:textSize="@dimen/text_size_17"
                    android:textColor="@color/colorLabel_label1"
                    android:id="@+id/title"
                    android:visibility="invisible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/club"
                    android:fontFamily="@font/inter_semibold_600"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView
                    android:id="@+id/imgSearch"
                    android:padding="@dimen/padding_16"
                    android:visibility="visible"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:foreground="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginEnd="@dimen/margin_16"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic_m__search_red"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.core.widget.NestedScrollView
                android:id="@+id/scrollView"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:fillViewport="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tobBar">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/club"
                        android:layout_marginHorizontal="@dimen/margin_20"
                        style="@style/Widget.MyStyle.text_Title"/>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:padding="@dimen/padding_12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/llCreateEvent"
                            android:padding="@dimen/padding_12"
                            android:focusable="true"
                            android:clickable="true"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:foreground="?attr/selectableItemBackgroundBorderless">
                            <ImageView
                                android:layout_width="28dp"
                                android:layout_height="28dp"
                                app:srcCompat="@drawable/ic_calendar"
                                app:tint="@color/colorPalette_red"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/create_event"
                                android:paddingStart="10dp"
                                style="@style/Widget.MyStyle.text_Label_1"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/llJoinById"
                            android:padding="@dimen/padding_12"
                            android:focusable="true"
                            android:clickable="true"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:foreground="?attr/selectableItemBackgroundBorderless">
                            <ImageView
                                android:layout_width="28dp"
                                android:layout_height="28dp"
                                app:srcCompat="@drawable/ic_user_plus"
                                app:tint="@color/colorPalette_red"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/join_via_event_id"
                                android:paddingHorizontal="10dp"
                                style="@style/Widget.MyStyle.text_Label_1"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/llMyEvents"
                            android:padding="@dimen/padding_12"
                            android:focusable="true"
                            android:clickable="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:foreground="?attr/selectableItemBackgroundBorderless">
                            <ImageView
                                android:layout_width="28dp"
                                android:layout_height="28dp"
                                app:srcCompat="@drawable/ic_history"
                                app:tint="@color/colorPalette_red"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/my_events"
                                android:paddingStart="10dp"
                                style="@style/Widget.MyStyle.text_Label_1"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/ll_ongoingWrap"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_20"
                            android:text="@string/my_ongoing"
                            style="@style/Widget.MyStyle.text_Heading_1"/>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <androidx.recyclerview.widget.RecyclerView
                                android:orientation="horizontal"
                                android:id="@+id/recyclerViewOngoingItems"
                                android:padding="8dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/ll_upcomingWrap"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_20"
                            android:text="@string/my_upcoming"
                            style="@style/Widget.MyStyle.text_Heading_1"/>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <androidx.recyclerview.widget.RecyclerView
                                    android:orientation="horizontal"
                                    android:id="@+id/recyclerViewUpcomingItems"
                                    android:padding="8dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/ll_virtualRaceWrap"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_20">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="Virtual Races"
                                android:layout_weight="1"
                                style="@style/Widget.MyStyle.text_Heading_1"/>
                            <ImageView
                                android:id="@+id/img_virtuslRacesMoreBtn"
                                android:layout_width="28dp"
                                android:layout_height="28dp"
                                app:srcCompat="@drawable/ic_options"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <androidx.recyclerview.widget.RecyclerView
                                android:orientation="horizontal"
                                android:id="@+id/recyclerViewVirtualRaceItems"
                                android:padding="8dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/ll_sharedGoalsWrap"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_20">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/shared_goals"
                                android:layout_weight="1"
                                style="@style/Widget.MyStyle.text_Heading_1"/>
                            <ImageView
                                android:id="@+id/img_shareGoalsMoreBtn"
                                android:layout_width="28dp"
                                android:layout_height="28dp"
                                app:srcCompat="@drawable/ic_options"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <androidx.recyclerview.widget.RecyclerView
                                android:orientation="horizontal"
                                android:id="@+id/recyclerViewsharedGoalItems"
                                android:padding="8dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/ll_rankingsWrap"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_20">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:text="@string/challenge_rankings"
                                android:layout_weight="1"
                                style="@style/Widget.MyStyle.text_Heading_1"/>
                            <ImageView
                                android:id="@+id/img_rankingsMoreBtn"
                                android:layout_width="28dp"
                                android:layout_height="28dp"
                                app:srcCompat="@drawable/ic_options"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <androidx.recyclerview.widget.RecyclerView
                                android:orientation="horizontal"
                                android:id="@+id/recyclerViewRankingItems"
                                android:padding="8dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/LL_virtualRaces"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/margin_20">
                        <TextView
                            android:textColor="@color/colorPalette_red"
                            android:id="@+id/tv_allEvent"
                            android:background="@drawable/bg_corner28_black_alpha_8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:foreground="?attr/selectableItemBackgroundBorderless"
                            android:text="@string/browse_all_events"
                            android:paddingHorizontal="28dp"
                            android:paddingVertical="16dp"
                            style="@style/Widget.MyStyle.text_Label_1"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
