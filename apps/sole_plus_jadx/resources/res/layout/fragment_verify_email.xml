<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/fragment_verify_email_0"
    android:background="@color/colorBackground_canvas"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/statusBarHeight"
        android:background="@color/colorBackground_canvas"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/top_bar_total_height"
        app:layout_collapseMode="parallax"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusBarHeight"
        app:layout_scrollFlags="exitUntilCollapsed">
        <ImageView
            android:id="@+id/back"
            android:padding="@dimen/padding_8"
            android:layout_width="@dimen/img_size_44"
            android:layout_height="@dimen/img_size_44"
            android:layout_marginStart="@dimen/margin_12"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_m__chevron_back"
            app:tint="@color/colorGlobal_accent"/>
        <ImageView
            android:id="@+id/endImg"
            android:padding="@dimen/padding_8"
            android:visibility="invisible"
            android:layout_width="@dimen/img_size_44"
            android:layout_height="@dimen/img_size_44"
            android:layout_marginEnd="@dimen/margin_12"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:textSize="@dimen/text_size_17"
            android:textColor="@color/colorLabel_label1"
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/verify_email"
            android:fontFamily="@font/inter_semibold_600"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/endImg"
            app:layout_constraintStart_toEndOf="@+id/back"
            app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.core.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topbar">
        <LinearLayout
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="@dimen/text_size_17"
                android:textColor="@color/colorLabel_label1"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:layout_marginBottom="@dimen/margin_16"
                android:text="@string/hint_4"
                android:fontFamily="@font/inter_regular_400"
                android:layout_marginStart="@dimen/margin_32"
                android:layout_marginEnd="@dimen/margin_32"/>
            <TextView
                android:textSize="@dimen/text_size_19"
                android:textColor="@color/colorLabel_accent"
                android:gravity="center"
                android:id="@+id/email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/inter_semibold_600"
                android:layout_marginStart="@dimen/margin_20"
                android:layout_marginEnd="@dimen/margin_20"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_36">
                <EditText
                    android:gravity="center"
                    android:id="@+id/edt_1"
                    android:background="@drawable/ic_field_digit"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:clickable="false"
                    android:longClickable="false"
                    android:layout_width="36dp"
                    android:layout_height="44dp"
                    android:maxLength="1"
                    android:inputType="number"
                    style="@style/Widget.MyStyle.text_number_S"/>
                <EditText
                    android:gravity="center"
                    android:id="@+id/edt_2"
                    android:background="@drawable/ic_field_digit"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:clickable="false"
                    android:longClickable="false"
                    android:layout_width="37dp"
                    android:layout_height="44dp"
                    android:maxLength="1"
                    android:inputType="number"
                    style="@style/Widget.MyStyle.text_number_S"/>
                <EditText
                    android:gravity="center"
                    android:id="@+id/edt_3"
                    android:background="@drawable/ic_field_digit"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:clickable="false"
                    android:longClickable="false"
                    android:layout_width="37dp"
                    android:layout_height="44dp"
                    android:maxLength="1"
                    android:inputType="number"
                    style="@style/Widget.MyStyle.text_number_S"/>
                <EditText
                    android:gravity="center"
                    android:id="@+id/edt_4"
                    android:background="@drawable/ic_field_digit"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:clickable="false"
                    android:longClickable="false"
                    android:layout_width="37dp"
                    android:layout_height="44dp"
                    android:maxLength="1"
                    android:inputType="number"
                    style="@style/Widget.MyStyle.text_number_S"/>
                <EditText
                    android:gravity="center"
                    android:id="@+id/edt_5"
                    android:background="@drawable/ic_field_digit"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:clickable="false"
                    android:longClickable="false"
                    android:layout_width="37dp"
                    android:layout_height="44dp"
                    android:maxLength="1"
                    android:inputType="number"
                    style="@style/Widget.MyStyle.text_number_S"/>
                <EditText
                    android:gravity="center"
                    android:id="@+id/edt_6"
                    android:background="@drawable/ic_field_digit"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:clickable="false"
                    android:longClickable="false"
                    android:layout_width="37dp"
                    android:layout_height="44dp"
                    android:maxLength="1"
                    android:inputType="number"
                    style="@style/Widget.MyStyle.text_number_S"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="100dp">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_8"
                    android:layout_marginEnd="@dimen/margin_8">
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number1"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:foreground="?attr/selectableItemBackgroundBorderless"
                        android:text="1"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number2"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="2"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number3"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="3"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/margin_4"
                    android:layout_marginStart="@dimen/margin_8"
                    android:layout_marginEnd="@dimen/margin_8">
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number4"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="4"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number5"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="5"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number6"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="6"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/margin_4"
                    android:layout_marginStart="@dimen/margin_8"
                    android:layout_marginEnd="@dimen/margin_8">
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number7"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="7"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number8"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="8"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number9"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="9"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/margin_4"
                    android:layout_marginStart="@dimen/margin_8"
                    android:layout_marginEnd="@dimen/margin_8">
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/clear"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/imageView18"
                            android:visibility="visible"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            app:srcCompat="@drawable/icon_dot"/>
                    </LinearLayout>
                    <TextView
                        android:textSize="@dimen/text_size_28"
                        android:textColor="@color/colorLabel_accent"
                        android:gravity="center"
                        android:id="@+id/number0"
                        android:background="@drawable/bg_corner32_raised"
                        android:padding="@dimen/padding_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:layout_weight="1"
                        android:fontFamily="@font/inter_bold_700"
                        android:layout_marginStart="@dimen/margin_4"
                        android:layout_marginEnd="@dimen/margin_4"/>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/delete"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/imageView19"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            app:srcCompat="@drawable/ic_m__clear_fill"
                            app:tint="@color/colorGlobal_accent"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/margin_32"
                    android:paddingStart="@dimen/padding_16"
                    android:paddingEnd="@dimen/padding_16">
                    <TextView
                        android:textSize="@dimen/text_size_15"
                        android:textColor="@color/colorLabel_label3"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/hint_5"
                        android:fontFamily="@font/inter_medium_500"/>
                    <com.google.android.flexbox.FlexboxLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:flexWrap="wrap"
                        app:justifyContent="center">
                        <TextView
                            android:textSize="@dimen/text_size_15"
                            android:textColor="@color/colorLabel_label3"
                            android:gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/hint_6"
                            android:fontFamily="@font/inter_medium_500"/>
                        <TextView
                            android:textColor="@color/colorPalette_red"
                            android:gravity="center"
                            android:id="@+id/resendCode"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/hint_6_1"
                            android:layout_marginStart="5dp"
                            style="@style/Widget.MyStyle.text_Label_2"/>
                        <TextView
                            android:textColor="@color/colorLabel_label3"
                            android:id="@+id/resendCodeDelayCount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_marginStart="5dp"
                            style="@style/Widget.MyStyle.text_Body_2"/>
                    </com.google.android.flexbox.FlexboxLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
